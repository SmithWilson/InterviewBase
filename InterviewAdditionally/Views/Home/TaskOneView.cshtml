@{
    ViewBag.Title = "Задание 1";
}

<h2>Задание 1 Простой</h2>

<table cellpadding="0" cellspacing="0" border="0" class="display" id="example"></table>
<script>
    $.ajax({

        type: "GET",
        url: 'http://localhost:60290/Home/TaskOne',
        dataType: 'json',

        success: function (obj, textstatus) {
            console.log(obj);
            var dataSet = new Array;
            if (!('error' in obj)) {
                $.each(obj, function (index, value) {
                    var tempArray = new Array;
                    for (var o in value) {
                        tempArray.push(value[o]);
                    }
                    dataSet.push(tempArray);
                })

                console.log(dataSet);

                $('#example').DataTable({
                    data: dataSet,
                    columns: [
                        { "title": "Имя" },
                        { "title": "Фаммлия" },
                        { "title": "Дата рождения" },
                        { "title": "Марка" }
                    ]
                });
            }
            else {
                alert(obj.error);
            }
        },
        error: function (obj, textstatus) {
            alert(obj.msg);
        }
    });
</script>

  